{
  "name": "simon",
  "version": "0.1.0",
  "homepage": "https://SharpEleven91.github.io/SimonGame",
  "private": true,
  "dependencies": {
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "jest-cli": "^21.2.1",
    "jquery": "^3.2.1",
    "node-sass-chokidar": "0.0.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
